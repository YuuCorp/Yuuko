{
  "name": "anisuggest",
  "type": "module",
  "version": "1.0.0",
  "description": "A bot that goes through your anilist and recommends you anime and manga.",
  "author": "Yuuko Devs",
  "license": "MIT",
  "keywords": [
    "anilist",
    "manga",
    "anime",
    "suggest"
  ],
  "main": "src/app.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --env production",
    "dev": "bun run --watch ./src/app.ts",
    "start:prod": "bun run ./src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "gen:gql": "graphql-codegen",
    "gen:queries": "bun run ./src/Scripts/genQueries.ts",
    "db:push:main": "bun drizzle-kit push:sqlite",
    "db:push:stats": " bun drizzle-kit push:sqlite --config=drizzle.stats.config.ts",
    "db:push": "bun run db:push:main && bun run db:push:stats",
    "db:generate:main": "bun drizzle-kit generate:sqlite",
    "db:generate:stats": "bun drizzle-kit generate:sqlite --config=drizzle.stats.config.ts",
    "db:generate": "bun run db:generate:main && bun run db:generate:stats",
    "db:studio": "bun drizzle-kit studio"
  },
  "trustedDependencies": [
    "better-sqlite3",
    "canvas",
    "sqlite3",
    "bindings"
  ],
  "dependencies": {
    "@acegoal07/discordjs-pagination": "^1.5.0",
    "@discordjs/rest": "^0.3.0",
    "@types/bun": "^1.0.7",
    "@types/dotenv-flow": "^3.3.1",
    "@types/ms": "^0.7.33",
    "@types/node-rsa": "^1.1.2",
    "@upstash/redis": "^1.23.4",
    "axios": "^0.21.1",
    "better-sqlite3": "^9.2.2",
    "bun-types": "^1.0.5",
    "colors": "^1.4.0",
    "discord.js": "^14.7.1",
    "dotenv-flow": "^3.2.0",
    "drizzle-orm": "^0.28.6",
    "elysia": "^0.8.17",
    "humanize-duration": "^3.27.0",
    "jimp": "^0.22.10",
    "ms": "^2.1.3",
    "node-redis": "^0.1.7",
    "node-rsa": "^1.1.1",
    "pm2": "^5.3.0",
    "redis": "^4.6.10",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "imports": {
    "#Utils/*": [
      "./src/Utils/*"
    ],
    "#Commands/*": [
      "./src/Commands/*"
    ],
    "#Structures/*": [
      "./src/Structures/*"
    ],
    "#GraphQL/*": [
      "./src/GraphQL/*"
    ],
    "#Models/*": [
      "./src/Database/Models/*"
    ],
    "#Caching/*": [
      "./src/Caching/*"
    ],
    "#RSA/*": [
      "./src/RSA/*"
    ],
    "#Middleware/*": [
      "./src/Middleware/*"
    ],
    "#Components/*": [
      "./src/Components/*"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.26",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/better-sqlite3": "^7.6.5",
    "@types/humanize-duration": "^3.27.1",
    "@types/node": "^20.6.3",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "typescript": "^5.2.2"
  }
}
